            <tr>
                <td>Потери после обработки</td>
                <td><input type="text" value="4%" class="input_base" name="OnlineOffice_first_table_2_9"></td>
            </tr>
            <tr>
                <td>Недозвонившиеся клиенты</td>
                <td><input type="text" value="1%" class="input_base" name="OnlineOffice_first_table_2_10"></td>
            </tr>
            <tr>
                <td>Потолок недозвоны</td>
                <td><input type="text" value="1" class="input_base" name="OnlineOffice_first_table_2_11"></td>
            </tr>
            <tr>
                <td>Потолок > 1 минуты</td>
                <td><input type="text" value="1" class="input_base" name="OnlineOffice_first_table_2_12"></td>
            </tr>
